{
  "_version": "1.0.0",
  "sap.app": {
    "id": "sapui5-init.app0",
    "type": "application",
    "i18n": "i18n/i18n.properties",
    "title": "{{appTitle}}",
    "description": "{{appDescription}}",
    "applicationVersion": {
      "version": "1.0.0"
    }
  },
  "sap.ui": {
    "technology": "UI5",
    "deviceTypes": {
      "desktop": true,
      "tablet": true,
      "phone": true
    }
  },
  "sap.ui5": {
    "rootView": {
      "viewName": "sapui5-init.app0.view.App",
      "type": "XML",
      "id": "App"
    },
    "dependencies": {
      "minUI5Version": "1.114.0",
      "libs": {
        "sap.ui.core": {},
        "sap.m": {}
      }
    },
    "models": {
      "i18n": {
        "type": "sap.ui.model.resource.ResourceModel",
        "settings": {
          "bundleName": "sapui5-init.app0.i18n.i18n",
          "supportedLocales": [""],
          "fallbackLocale": ""
        }
      }
    }
  }
}
